PREFIX wdata: <https://www.wikidata.org/wiki/Special:EntityData/> .
PREFIX cc: <http://creativecommons.org/ns#>
start=@<wikidata_item>



<wikipedia_article> {
	a [schema:Article] ;
	schema:about @<#wikidata_item> ;
	schema:isPartOf @<#wikimedia_project> ;
      schema:inLanguage @<#wikimedia_supported_language> ;
      schema:name rdf:langLabel ;
}

<#wdata> {
     a [schema:Dataset];
     schema:about @<#wikidata_item> ;
     cc:license   [<http://creativecommons.org/publicdomain/zero/1.0/>];
     schema:softwareVersion xsd:string ;
     schema:version xsd:integer ;
     schema:dateModified xsd:dateTime ;
     wikibase:statements xsd:integer ;
     wikibase:identifiers xsd:integer ;
     wikibase:sitelinks xsd:integer ;
}


<#wikidata_item> {
      rdfs:label rdf:langLabel ;
      skos:prefLabel rdf:langLabel ;
      schema:name rdf:langLabel ;
      schema:description rdf:langLabel ;
      skos:altLabel rdf:langLabel ;
}

