#Shape Expression for software on Wikidata

#BASE <http://base.example/#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX wd: <http://www.wikidata.org/wiki/>
PREFIX ent: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>

PREFIX ps: <http://www.wikidata.org/prop/statement/>

start = @<#wikidata-software>

<#wikidata-software> {
	#statements
	
	
	p:275 @<#P275_license> ;
	
	p:348 @<#P348_software_version> ;
	
	P:856 @<#P856_official_website> ;
	
	p:178 @<#P178_developer> ; 
	
	p:306 @<#P306_operating_system> ;
 
 	p:1072 @<#P1072_readable_file_format> ;
 
 	p:1073 @<#P1073_writable_file_format> ;
	
	p:144  @<#P144_based_on>* ;
	
	p:1547 @<#P1547_depends_on_software>* ;
	
	p:571 @<#P571_inception>* ;
	
	p:277 @<#P277_programming_language> ;
	
	p:577 @<#P577_publication_date>* ;
 
 	p:1324 @<#P1324_source_code_repository>* ;
	
	p:2078 @<#P2078_user_manual_link>* ;
 	
	
	#identifiers
	p:2749 @<#P2749_PRONOM_software_identifier>* ;
  
  	p:4107 @<#P4107_Framalibre_ID>* ;
	
	p:1972 @<#P1972_Open_Hub_ID>* ;
	
	p:3112 @<#P3112_DistroWatch_id>* ;
	
	p:3597 @<#P3597_F-Droid_package>* ;
	
	p:2537 @<#P2537_Free_Software_Directory_entry>* ;
	
	p:356 @<#P356_DOI>* ;
}

# Shape expression for Wikidata statements related to software


<#P275_license> {
	ps:P275 IRI;
}

<#P348_software_version> {
	ps:P348 LITERAL;
}

<#P856_official_website> {
	ps:P856 LITERAL;
}

<#P178_developer> {
	ps:P178 IRI;
}

<#P1072_readable_file_format> {
	ps:P1072 IRI;
}

<#P1073_writable_file_format> {
	ps:1073 IRI;
}

<#P306_operating_system> {
	ps:P306 IRI;
}

<#P2749_PRONOM_software_id> {
	ps:P2749 LITERAL;
}

<#P4107_Framalibre_ID> {
  ps:P4107 LITERAL;
}

<#P1972_Open_Hub_ID> {
  ps:P1972 LITERAL;
}

<#P144_based_on> {
  ps:P144 IRI;
}

<#P1547_depends_on_software> {
  ps:P1547 IRI;
}

<#P571_inception> {
  ps:P571 IRI;
}
	
<#P277_programming_language> {
  ps:P277 IRI;
}		

<#P577_publication_date> {
  ps:P577 LITERAL;
}

<#P1324_source_code_repository> {
  ps:P1324 LITERAL;
}

<#P2078_user_manual_link> {
  ps:P2078 LITERAL;
}

	
